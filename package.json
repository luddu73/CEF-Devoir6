{
  "private": true,
  "scripts": {
    "start": "npm run server & npm run client",
    "server": "cd back && npm run prod",
    "server-dev": "cd back && npm run dev",
    "client": "cd front && npm run build",
    "client-dev": "cd front && npm run start",
    "prod": "concurrently \"npm run server\" \"npm run client\"",
    "dev": "concurrently \"npm run server-dev\" \"npm run client-dev\""
  },
  "dependencies": {
    "concurrently": "^9.1.2"
  }
}
